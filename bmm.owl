    <!-- http://www.semanticweb.org/ontologies/bmm#Assessment -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/bmm#Assessment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/bmm#Concept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/bmm#expressesImpactOf"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/bmm#isJudgmentOf"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/bmm#identifies"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:description xml:lang="en">An Assessment is a judgment that an influencer affects the employment of means and/or the achievement of ends.</terms:description>
        <rdfs:comment xml:lang="en">Categories of Assessment include: Opportunity, Strength, Threat and Weakness. An assessment expresses a logical connection between influencers and the ends or means of the business plans.</rdfs:comment>
        <rdfs:label xml:lang="en">Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/bmm#Assumption -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/bmm#Assumption">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/bmm#InternalInfluencer"/>
        <terms:description xml:lang="en">Internal Influencer that is taken for granted or without proof.</terms:description>
        <rdfs:comment xml:lang="en">A thing that is accepted as true or as certain to happen, without proof.</rdfs:comment>
        <rdfs:label xml:lang="en">Assumption</rdfs:label>
    </owl:Class>
    


